### Spring profile (your app loads `.env.<profile>` automatically)
SPRING_PROFILES_ACTIVE=
SPRING_APPLICATION_NAME=re-shopping-cart-web-app

### Server
SERVER_PORT=8080

### External MySQL (Aiven)
DB_URL=
DB_USERNAME=
DB_PASSWORD=

### Optional tuning (leave as defaults if unsure)
DB_CONN_TIMEOUT_MS=30000
DB_POOL_MAX_SIZE=10
DB_POOL_MIN_IDLE=5
JPA_SHOW_SQL=false
THYMELEAF_CACHE=false
